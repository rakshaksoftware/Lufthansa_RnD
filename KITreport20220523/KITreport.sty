    \pgfsetstrokecolor{gray}%
    \pgfsetlinewidth{\KIT@framewidth}%
    \pgfsetcornersarced{\pgfpoint{\KIT@radius}{\KIT@radius}}%
    \pgfpathmoveto{\pgfpoint{\KIT@urx}{0mm}}%
    \pgfpathlineto{\pgfpoint{\KIT@urx}{\KIT@ury}}%
